{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
    "warn",
    {
      "singleQuote": true,
      "tabWidth": 4,
      "parser": "flow",
      "endOfLine": "auto"
    }
  ],
    "no-console":"off",
    "no-underscore-dangle":"off",
    "no-unused-vars":"warn",
    "consistent-return":"warn",
    "spaced-comment":"warn"
  }
}
